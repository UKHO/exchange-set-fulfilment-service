{
    "local": {
        "UKHO.ADDS.EFS.Orchestrator": {
            "Logging": {
                "LogLevel": {
                    "Default": "Information",
                    "Microsoft.AspNetCore": "Warning",
                    "Aspire.Hosting.Dcp": "Warning"
                }
            },
            "Endpoints": {
                "FileShare": "{{mockfss}}", // Local dynamic fss mock address from aspire
                "SalesCatalogue": "{{mockscs}}" // Local dynamic scs mock address from aspire
            },
            "AllowedHosts": "*",
            "BuilderContainerTag": "latest",
            "JobRequestQueue": {
                "BatchSize": 1,
                "PollingIntervalSeconds": 1,
                "ChannelSize": 10
            },
            "Builders": {
                "MaximumConcurrentBuilders": 5
            }
        },
        "UKHO.ADDS.EFS.Builder.S100": {
            "Endpoints": {
                "S100BuilderFileShare": "{{buildermockfss}}" // Local dynamic fss mock address from aspire with host adjusted for docker 
            },
            "IICWorkspaceKey": "D89D11D265B19CA5C2BE97A7FCB1EF21", // Secret has literal value in local environment only
            "ExchangeSetNameTemplate": "ExchangeSet_[jobid]" // Template for naming an exchange set: options are jobid, ...
        }
    },
