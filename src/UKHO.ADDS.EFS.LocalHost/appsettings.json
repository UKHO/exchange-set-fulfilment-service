{
    "Logging": {
        "LogLevel": {
            "Default": "Information",
            "Microsoft.AspNetCore": "Warning",
            "Aspire.Hosting.Dcp": "Warning"
        }
    },
    "Endpoints": {
        "WASBlobPort": 10000, // Blob emulator port
        "WASQueuePort": 10001, // Queue emulator port
        "WASTablePort": 10002, // Table emulator port
        "MockEndpointPort": 5679, // ADDS Mock port
        "MockEndpointContainerPort": 5678 // Internal port specified in the docker file
    },
    "Orchestrator": {
        "BuilderStartup": "Orchestrator" // Orchestrator container start behaviour (see docs)
    },
    "Telemetry": {
        "ExposeOtlp": true // Start the Prometheus exporter and Grafana dashboard
    },
    "Containers": {
        "ContainerRuntime": "docker" // Set to either "docker" or "podman" depending on what is installed locally
    }
}
