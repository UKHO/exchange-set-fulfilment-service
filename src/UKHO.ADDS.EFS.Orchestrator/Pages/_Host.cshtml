@page "/"
@using Microsoft.AspNetCore.Mvc.TagHelpers
@using UKHO.ADDS.EFS.Orchestrator
@namespace UKHO.ADDS.EFS.Orchestrator.Dashboard.Server.Pages
@addTagHelper *, Microsoft.AspNetCore.Mvc.TagHelpers
@{
    Layout = null;
}

<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>UKHO.ADDS.EFS.Orchestrator.Dashboard</title>
    <base href="~/" />

   
    <link rel="stylesheet" href="@("https://unpkg.com/@tabler/core@1.0.0-beta21/dist/css/tabler.min.css")"> 
    
    <link rel="stylesheet" href="_content/TabBlazor/css/tabblazor.min.css?v=@ThisAssembly.AssemblyFileVersion" />
    <script src="_content/TabBlazor/js/tabblazor.js?v=@ThisAssembly.AssemblyFileVersion"></script>

    <link href="UKHO.ADDS.EFS.Orchestrator.Dashboard.Server.styles.css" rel="stylesheet" />
    <link href="_content/UKHO.ADDS.EFS.Orchestrator.Dashboard/css/tabler-docs.css" rel="stylesheet" />
</head>
<body data-bs-theme="">
    <component type="typeof(App)" render-mode="ServerPrerendered" />

    <div id="blazor-error-ui">
        <environment include="Staging,Production">
            An error has occurred. This application may no longer respond until reloaded.
        </environment>
        <environment include="Development">
            An unhandled exception has occurred. See browser dev tools for details.
        </environment>
        <a href="" class="reload">Reload</a>
        <a class="dismiss">🗙</a>
    </div>

    <script src="_framework/blazor.server.js"></script>
</body>
</html>
