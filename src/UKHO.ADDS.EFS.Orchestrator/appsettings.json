{
    "Logging": {
        "LogLevel": {
            "Default": "Information",
            "Microsoft": "Warning",
            "Azure.Core": "Warning",
            "Azure.Storage.Queues": "Warning",
            "Azure.Storage.Blobs": "Critical"
        }
    },
    "Serilog": {
        "MinimumLevel": {
            "Default": "Information",
            "Override": {
                "Microsoft": "Warning",
                "Microsoft.AspNetCore.Hosting.Diagnostics": "Error",
                "Microsoft.Hosting.Lifetime": "Information",
                "Azure.Core": "Warning",
                "Azure.Storage.Queues": "Warning",
                "Azure.Storage.Blobs": "Fatal"
            }
        },
        "WriteTo": [
            {
                "Name": "Console",
                "Args": {
                    "outputTemplate": "{Timestamp:yyyy-MM-dd HH:mm:ss} [{Level}] [{SourceContext}] {Message}{NewLine}{Exception}"
                }
            }
        ]
    },
    "AllowedHosts": "*",
    "BuilderContainerTag": "latest",
    "QueuePolling": {
        "BatchSize": 10,
        "ChannelSize": 20,
        "PollingIntervalSeconds": 5
    },
    "Builders": {
        "MaximumConcurrentBuilders": 5
    }
}
